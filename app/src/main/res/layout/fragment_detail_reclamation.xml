<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.DetailReclamationFragment">
    <LinearLayout
        android:id="@+id/reclamation_fragment_layout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ListView
            android:id="@+id/reclamationList"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:dividerHeight="1dp"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/reclamation_fragment_layout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/pas_reclamation_textview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Pas de reclamation"
            android:textAlignment="center"/>
    </LinearLayout>
</LinearLayout>